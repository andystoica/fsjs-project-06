{
  "name": "fsjs-project-06",
  "version": "0.0.1",
  "description": "A content scrapper",
  "main": "scraper.js",
  "scripts": {
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andystoica/fsjs-project-06.git"
  },
  "author": "Andy Stoica",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andystoica/fsjs-project-06/issues"
  },
  "homepage": "https://github.com/andystoica/fsjs-project-06#readme",
  "dependencies": {
    "json2csv": "^3.7.0",
    "node-scrapy": "^0.3.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "array-bracket-spacing": [2, "never"],
      "block-scoped-var": 2,
      "brace-style": [2, "1tbs"],
      "camelcase": 1,
      "computed-property-spacing": [2, "never"],
      "curly": 2,
      "eol-last": 2,
      "eqeqeq": [2, "smart"],
      "max-depth": [1, 3],
      "max-len": [1, 80],
      "max-statements": [1, 15],
      "new-cap": 1,
      "no-extend-native": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-trailing-spaces": 2,
      "no-unused-vars": 1,
      "no-use-before-define": [2, "nofunc"],
      "object-curly-spacing": [2, "never"],
      "quotes": [2, "single", "avoid-escape"],
      "semi": [2, "always"],
      "keyword-spacing": [2, {"before": true, "after": true}],
      "space-unary-ops": 2
    }
  },
  "scripts": {
    "lint": "eslint scraper.js"
  }
}
